{
  "extends": "@rocketseat/eslint-config/react",
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "semi": false,
        "singleQuote": true,
        "jsxSingleQuote": true,
        "arrowParens": "avoid",
        "endOfLine": "auto"
      }
    ],
    "no-unused-vars": "warn"
  }
}
